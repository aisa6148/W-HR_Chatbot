{
  "name": "whrbot",
  "version": "1.0.0",
  "description": "W-HR Chatbot",
  "main": "dist",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc && npm run copy-static-assets",
    "copy-static-assets": "ts-node copy-static-assets.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "watch": "concurrently  \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "npm run lint-fix && npm run copy-static-assets && tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://registry.npmjs.org"
  },
  "keywords": [
    "chatbot",
    "w-hr",
    "hr"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-queue": "^12.3.1",
    "@sendgrid/mail": "^6.4.0",
    "@types/request": "^2.48.4",
    "applicationinsights": "^1.8.7",
    "azure-keyvault": "^3.0.4",
    "bluebird": "^3.5.3",
    "botbuilder": "^4.4.0",
    "botbuilder-azure": "^4.4.0",
    "botbuilder-dialogs": "^4.4.0",
    "botframework-connector": "^4.10.0",
    "botframework-schema": "^4.10.0",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.3",
    "cron": "^1.6.0",
    "date-diff": "^0.2.1",
    "debug": "~3.1.0",
    "documentdb": "^1.14.5",
    "eslint": "^5.16.0",
    "express": "^4.17.1",
    "hbs": "^4.0.3",
    "http-errors": "^1.7.3",
    "lodash": "^4.17.15",
    "make-promises-safe": "^4.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.17",
    "morgan": "^1.9.1",
    "ms-rest-azure": "^2.5.9",
    "mssql": "^4.1.0",
    "nodemailer": "^4.6.5",
    "querystring": "^0.2.0",
    "redis": "^2.8.0",
    "request": "^2.87.0",
    "shelljs": "^0.8.3",
    "source-map-support": "^0.5.12",
    "tedious": "^2.7.1",
    "tedious-connection-pool": "^1.0.5",
    "tslint-config-prettier": "^1.18.0",
    "util": "^0.11.1",
    "uuid": "^3.3.2",
    "uuid-token-generator": "^1.0.0",
    "winston": "^2.4.4",
    "winston-azure-blob-transport": "^0.2.11",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/cookie-parser": "^1.4.1",
    "@types/cron": "^1.7.1",
    "@types/express": "^4.17.0",
    "@types/http-errors": "^1.6.1",
    "@types/lodash": "^4.14.168",
    "@types/moment-timezone": "^0.5.12",
    "@types/morgan": "^1.7.35",
    "@types/node-fetch": "^2.3.6",
    "@types/redis": "^2.8.13",
    "@types/shelljs": "^0.8.5",
    "@types/tedious": "^4.0.0",
    "@types/tedious-connection-pool": "^1.0.2",
    "@types/uuid": "^3.4.4",
    "@types/xml2js": "^0.4.4",
    "dotenv": "^8.2.0",
    "nodemon": "^1.19.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  }
}
